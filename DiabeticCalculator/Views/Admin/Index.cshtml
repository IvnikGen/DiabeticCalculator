
@{
    ViewBag.Title = "Admin Panel";
    Layout = "~/Views/Shared/LayoutAdminPanel.cshtml";
}

    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Администраторы и пользователи</a>
            </li>
            <li class="breadcrumb-item active">Просмотр</li>
        </ol>
        <!-- Area Chart Example-->
        <div class="card mb-3">
            <div class="modal fade right" id="EditModalAdm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-full-height modal-right" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Редактирование пользователя</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body" id="bodymodalEdAdm">
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade right" id="DeleteModalAdm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-full-height modal-right" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Удаление пользователя</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body" id="bodymodalDel">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- DataTables Example -->
        <div class="card mb-3">
            @Html.Action("GetAllUsPartial")
        </div>
    </div>
    <!-- /.container-fluid -->

